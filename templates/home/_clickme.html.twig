<!-- Modal -->
<div class="modal fade" id="newsletterModal" tabindex="-1" role="dialog" aria-labelledby="Inscription à la newsletter" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="newsletterRegistration">{{ "S'incrire à la newletter" }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                {{ form_start(newsletterForm) }}

                <div class="form-group row">
                    <div class="col-6">{{ form_label(newsletterForm.email, 'Merci d\'indiquer votre email') }}</div>
                    <div class="col-sm-12 col-md-6">{{ form_widget(newsletterForm.email, {'attr': {'class': 'w-100'}}) }}</div>
                    <div class="col-12">{{ form_errors(newsletterForm.email) }}</div>
                </div>

                {% include 'tools/_backButton.html.twig' with {
                    'text': 'Annuler',
                } %}

                {% include 'tools/_formAnonymValidateButton.html.twig' with {
                    'text': 'Enregistrer',
                } %}

                {{ form_end(newsletterForm) }}

            </div>
        </div>
    </div>
</div>
