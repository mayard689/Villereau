{% extends 'base.html.twig' %}

{% block title %}ReportSubject index{% endblock %}

{% block body %}
    <h1>ReportSubject index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for report_subject in report_subjects %}
            <tr>
                <td>{{ report_subject.id }}</td>
                <td>{{ report_subject.title }}</td>
                <td>
                    <a href="{{ path('report_subject_show', {'id': report_subject.id}) }}">show</a>
                    <a href="{{ path('report_subject_edit', {'id': report_subject.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('report_subject_new') }}">Create new</a>
{% endblock %}
